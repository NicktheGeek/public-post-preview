    "@babel/preset-env": "^7.5.5",
    "@wordpress/babel-preset-default": "^4.3.0",
    "@wordpress/scripts": "^3.3.0",
    "babel-plugin-emotion": "^10.0.14",
    "emotion": "^10.0.14",
    "terser": "^4.1.2",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "scripts": {
    "build:legacy": "terser js/public-post-preview.js -o js/public-post-preview.min.js -c -m",
